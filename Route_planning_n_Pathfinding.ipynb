    "import pygame\n",
    "import sys\n",
    "import heapq\n",
    "\n",
    "# Initialize Pygame\n",
    "pygame.init()\n",
    "\n",
    "# Colors\n",
    "WHITE = (255, 255, 255)\n",
    "BLACK = (0, 0, 0)\n",
    "GREEN = (0, 255, 0)\n",
    "RED = (255, 0, 0)\n",
    "BLUE = (100, 100, 255)\n",
    "CYAN = (0, 255, 255)\n",
    "\n",
    "# Grid cell size and margins\n",
    "WIDTH = 22\n",
    "HEIGHT = 22\n",
    "MARGIN = 3\n",
    "\n",
    "# Grid size\n",
    "ROWS = 20\n",
    "COLS = 20\n",
    "\n",
    "# Window size\n",
    "WINDOW_SIZE = [(WIDTH + MARGIN) * COLS + MARGIN,\n",
    "               (HEIGHT + MARGIN) * ROWS + MARGIN]\n",
    "screen = pygame.display.set_mode(WINDOW_SIZE)\n",
    "pygame.display.set_caption(\"Task 1.2 & 1.3 - A* Pathfinding\")\n",
    "\n",
    "# === Field Class ===\n",
    "class Field:\n",
    "    def __init__(self, row, col):\n",
    "        self.row = row\n",
    "        self.col = col\n",
    "        self.color = WHITE\n",
    "        self.wall = False\n",
    "\n",
    "        # A* values\n",
    "        self.g = float('inf')\n",
    "        self.h = 0\n",
    "        self.f = float('inf')\n",
    "        self.parent = None\n",
    "\n",
    "    def draw(self):\n",
    "        pygame.draw.rect(\n",
    "            screen,\n",
    "            self.color,\n",
    "            [(MARGIN + WIDTH) * self.col + MARGIN,\n",
    "             (MARGIN + HEIGHT) * self.row + MARGIN,\n",
    "             WIDTH,\n",
    "             HEIGHT]\n",
    "        )\n",
    "\n",
    "# === Heuristic function ===\n",
    "def heuristic(a, b):\n",
    "    return abs(a.row - b.row) + abs(a.col - b.col)  # Manhattan distance\n",
    "\n",
    "# === Grid Class ===\n",
    "class Grid:\n",
    "    def __init__(self, rows, cols):\n",
    "        self.rows = rows\n",
    "        self.cols = cols\n",
    "        self.grid = [[Field(r, c) for c in range(cols)] for r in range(rows)]\n",
    "        self.add_default_layout()\n",
    "\n",
    "    def add_default_layout(self):\n",
    "        # Match the assignment's visual layout\n",
    "\n",
    "        # Vertical wall (Column 10, Rows 2 to 10)\n",
    "        for row in range(1, 10):\n",
    "            self.grid[ROWS - row - 1][9].color = BLACK\n",
    "            self.grid[ROWS - row - 1][9].wall = True\n",
    "\n",
    "        # Horizontal wall (Row 10, Columns 5 to 10)\n",
    "        for col in range(4, 10):\n",
    "            self.grid[ROWS - 10][col].color = BLACK\n",
    "            self.grid[ROWS - 10][col].wall = True\n",
    "\n",
    "        # Right vertical wall (Column 18, Rows 11 to 20)\n",
    "        for row in range(10, 20):\n",
    "            self.grid[ROWS - row - 1][17].color = BLACK\n",
    "            self.grid[ROWS - row - 1][17].wall = True\n",
    "\n",
    "        # Start = bottom-left (1,1)\n",
    "        self.grid[19][0].color = GREEN\n",
    "\n",
    "        # Goal = top-right (20,20)\n",
    "        self.grid[0][19].color = RED\n",
    "\n",
    "    def draw(self):\n",
    "        for row in self.grid:\n",
    "            for field in row:\n",
    "                field.draw()\n",
    "\n",
    "    def get_neighbors(self, node):\n",
    "        neighbors = []\n",
    "        directions = [(0, 1), (1, 0), (-1, 0), (0, -1)]  # right, down, up, left\n",
    "        for dr, dc in directions:\n",
    "            r, c = node.row + dr, node.col + dc\n",
    "            if 0 <= r < self.rows and 0 <= c < self.cols:\n",
    "                neighbor = self.grid[r][c]\n",
    "                if not neighbor.wall:\n",
    "                    neighbors.append(neighbor)\n",
    "        return neighbors\n",
    "\n",
    "    def reconstruct_path(self, end):\n",
    "        current = end\n",
    "        while current.parent:\n",
    "            if current.color not in (GREEN, RED):\n",
    "                current.color = CYAN  # highlight path\n",
    "            current = current.parent\n",
    "\n",
    "    def a_star(self, start, goal):\n",
    "        open_set = []\n",
    "        heapq.heappush(open_set, (0, id(start), start))\n",
    "        start.g = 0\n",
    "        start.f = heuristic(start, goal)\n",
    "\n",
    "        while open_set:\n",
    "            current = heapq.heappop(open_set)[2]\n",
    "\n",
    "            if current == goal:\n",
    "                self.reconstruct_path(goal)\n",
    "                return True  # path found\n",
    "\n",
    "            for neighbor in self.get_neighbors(current):\n",
    "                tentative_g = current.g + 1\n",
    "                if tentative_g < neighbor.g:\n",
    "                    neighbor.parent = current\n",
    "                    neighbor.g = tentative_g\n",
    "                    neighbor.h = heuristic(neighbor, goal)\n",
    "                    neighbor.f = neighbor.g + neighbor.h\n",
    "                    if neighbor.color not in (GREEN, RED):\n",
    "                        neighbor.color = BLUE  # mark as visited\n",
    "                    heapq.heappush(open_set, (neighbor.f, id(neighbor), neighbor))\n",
    "\n",
    "        return False  # no path found\n",
    "\n",
    "# === Setup ===\n",
    "grid = Grid(ROWS, COLS)\n",
    "start = grid.grid[19][0]\n",
    "goal = grid.grid[0][19]\n",
    "path_found = False\n",
    "\n",
    "# === Main Loop ===\n",
    "running = True\n",
    "clock = pygame.time.Clock()\n",
    "\n",
    "while running:\n",
    "    for event in pygame.event.get():\n",
    "        if event.type == pygame.QUIT:\n",
    "            running = False\n",
    "\n",
    "    # Run A* once\n",
    "    if not path_found:\n",
    "        path_found = grid.a_star(start, goal)\n",
    "\n",
    "    # Draw\n",
    "    screen.fill(BLACK)\n",
    "    grid.draw()\n",
    "    pygame.display.flip()\n",
    "    clock.tick(60)\n",
    "\n",
    "pygame.quit()\n",
    "sys.exit()\n"
   ]
 
