    "from prettytable import PrettyTable\n",
    "import heapq\n",
    "\n",
    "# Utility to get a node by name\n",
    "def getNode(name, nodes):\n",
    "    return next((node for node in nodes if node.name == name), None)\n",
    "\n",
    "class Node:\n",
    "    def __init__(self, name):\n",
    "        self.parent = None\n",
    "        self.name = name\n",
    "        self.edges = []\n",
    "        self.value = 0\n",
    "\n",
    "class Edge:\n",
    "    def __init__(self, edge):\n",
    "        self.start = edge[0]\n",
    "        self.end = edge[1]\n",
    "        self.value = edge[2]\n",
    "\n",
    "class Graph:\n",
    "    def __init__(self, names, edges):\n",
    "        self.nodes = [Node(name) for name in names]\n",
    "        for (a, b, w) in edges:\n",
    "            n1 = getNode(a, self.nodes)\n",
    "            n2 = getNode(b, self.nodes)\n",
    "            n1.edges.append(Edge((n1, n2, w)))\n",
    "            n2.edges.append(Edge((n2, n1, w)))\n",
    "\n",
    "class Queue:\n",
    "    def __init__(self, mode='fifo'):\n",
    "        self.mode = mode\n",
    "        self.data = []\n",
    "    def push(self, item):\n",
    "        if self.mode == 'prio':\n",
    "            heapq.heappush(self.data, (item.value, item))\n",
    "        else:\n",
    "            self.data.append(item)\n",
    "    def pop(self):\n",
    "        if self.mode == 'fifo':\n",
    "            return self.data.pop(0)\n",
    "        elif self.mode == 'lifo':\n",
    "            return self.data.pop()\n",
    "        else:\n",
    "            return heapq.heappop(self.data)[1]\n",
    "    def is_empty(self):\n",
    "        return len(self.data) == 0\n",
    "\n",
    "def calculate_cost(graph, path):\n",
    "    cost = 0\n",
    "    for i in range(len(path) - 1):\n",
    "        node = getNode(path[i], graph.nodes)\n",
    "        for e in node.edges:\n",
    "            if e.end.name == path[i+1]:\n",
    "                cost += e.value\n",
    "                break\n",
    "    return cost\n",
    "\n",
    "def bfs(graph, start, goal):\n",
    "    for n in graph.nodes:\n",
    "        n.parent = None\n",
    "    start_node = getNode(start, graph.nodes)\n",
    "    queue = Queue('fifo')\n",
    "    queue.push(start_node)\n",
    "    visited = set()\n",
    "    while not queue.is_empty():\n",
    "        curr = queue.pop()\n",
    "        if curr.name == goal:\n",
    "            path = []\n",
    "            node = curr\n",
    "            while node:\n",
    "                path.append(node.name)\n",
    "                node = node.parent\n",
    "            return list(reversed(path)), calculate_cost(graph, list(reversed(path)))\n",
    "        visited.add(curr.name)\n",
    "        for e in curr.edges:\n",
    "            if e.end.name not in visited and e.end.parent is None:\n",
    "                e.end.parent = curr\n",
    "                queue.push(e.end)\n",
    "    return None, 0\n",
    "\n",
    "def dfs(graph, start, goal):\n",
    "    for n in graph.nodes:\n",
    "        n.parent = None\n",
    "    start_node = getNode(start, graph.nodes)\n",
    "    stack = Queue('lifo')\n",
    "    stack.push(start_node)\n",
    "    visited = set()\n",
    "    while not stack.is_empty():\n",
    "        curr = stack.pop()\n",
    "        if curr.name == goal:\n",
    "            path = []\n",
    "            node = curr\n",
    "            while node:\n",
    "                path.append(node.name)\n",
    "                node = node.parent\n",
    "            return list(reversed(path)), calculate_cost(graph, list(reversed(path)))\n",
    "        visited.add(curr.name)\n",
    "        for e in curr.edges:\n",
    "            if e.end.name not in visited and e.end.parent is None:\n",
    "                e.end.parent = curr\n",
    "                stack.push(e.end)\n",
    "    return None, 0\n",
    "\n",
    "def ucs(graph, start, goal):\n",
    "    for n in graph.nodes:\n",
    "        n.parent = None\n",
    "        n.value = float('inf')\n",
    "    start_node = getNode(start, graph.nodes)\n",
    "    start_node.value = 0\n",
    "    visited = set()\n",
    "    queue = Queue('prio')\n",
    "    queue.push(start_node)\n",
    "    while not queue.is_empty():\n",
    "        curr = queue.pop()\n",
    "        if curr.name in visited:\n",
    "            continue\n",
    "        visited.add(curr.name)\n",
    "        if curr.name == goal:\n",
    "            path = []\n",
    "            node = curr\n",
    "            while node:\n",
    "                path.append(node.name)\n",
    "                node = node.parent\n",
    "            return list(reversed(path)), curr.value\n",
    "        for e in curr.edges:\n",
    "            new_cost = curr.value + e.value\n",
    "            if e.end.name not in visited and new_cost < e.end.value:\n",
    "                e.end.value = new_cost\n",
    "                e.end.parent = curr\n",
    "                queue.push(e.end)\n",
    "    return None, float('inf')\n",
    "\n",
    "# Construct Romania graph\n",
    "romania = Graph(\n",
    "    ['Or','Ne','Ze','Ia','Ar','Si','Fa','Va','Ri','Ti','Lu','Pi','Ur','Hi','Me','Bu','Dr','Ef','Cr','Gi'],\n",
    "    [\n",
    "      ('Or','Ze',71),('Or','Si',151),('Ne','Ia',87),('Ze','Ar',75),('Ia','Va',92),\n",
    "      ('Ar','Si',140),('Ar','Ti',118),('Si','Fa',99),('Si','Ri',80),('Fa','Bu',211),\n",
    "      ('Va','Ur',142),('Ri','Pi',97),('Ri','Cr',146),('Ti','Lu',111),('Lu','Me',70),\n",
    "      ('Me','Dr',75),('Dr','Cr',120),('Cr','Pi',138),('Pi','Bu',101),('Bu','Gi',90),\n",
    "      ('Bu','Ur',85),('Ur','Hi',98),('Hi','Ef',86)\n",
    "    ]\n",
    ")\n"
